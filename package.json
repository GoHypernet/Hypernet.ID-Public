{
  "name": "@hypernetlabs/hypernet-id-public",
  "version": "0.0.1",
  "description": "Hypernet.ID Public Code Monorepo",
  "private": true,
  "engines": {
    "node": ">= 14.x.x"
  },
  "author": "Charlie Sibbach",
  "keywords": [
    "Hypernet Labs",
    "Hypernet.ID",
    "Documentation",
    "Solidity"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "alias": "lerna run alias",
    "build": "yarn clean && yarn compile && yarn postCompile",
    "clean": "rimraf ./packages/*/dist ./packages/*/tsconfig.tsbuildinfo",
    "clean-docker": "docker-compose down",
    "clean-node": "rimraf node_modules packages/*/node_modules yarn.lock",
    "compile": "tsc --build && yarn alias",
    "dev": "docker-compose up -d redis && docker-compose up -d blockchain && cd packages/development && yarn start",
    "dev-server-only": "cd packages/development && yarn start",
    "docker-push": "lerna run docker-push",
    "dockerize": "lerna run dockerize",
    "format": "prettier . --write && yarn format:pkg",
    "format:pkg": "format-package -w",
    "generate-docs": "lerna run generate-docs",
    "lint": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet --fix",
    "postCompile": "lerna run postCompile",
    "start": "docker-compose up",
    "test": "lerna run test"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.14.0",
    "**/@typescript-eslint/parser": "^4.22.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.5",
    "@types/redis": "^2.8.32",
    "@types/redlock": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.22.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "buidler-extract": "^1.0.0",
    "buidler-gas-reporter": "^0.1.4",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.0.1",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "format-package": "^6.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "http-server": "^0.12.3",
    "https-browserify": "^1.0.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.4.2",
    "jest-cli": "^25.1.0",
    "jest-when": "^3.0.1",
    "lerna": "^4.0.0",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "^0.59.0",
    "moleculer-repl": "^0.6.2",
    "monaco-editor-webpack-plugin": "2.0.0",
    "node-sass": "4.13.0",
    "os-browserify": "^0.3.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.0",
    "source-map-loader": "^2.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "testdouble": "^3.16.2",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.3.9",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typedoc": "^0.20.16",
    "typescript": "^4.2.4",
    "url-loader": "3.0.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  }
}
